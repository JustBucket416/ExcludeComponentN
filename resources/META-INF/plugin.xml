<idea-plugin>
    <id>justbucket.excludeComponentN</id>
    <name>Exclude ComponentN</name>
    <version>0.2</version>
    <vendor email="26853613+JustBucket416@users.noreply.github.com">JustBucket</vendor>

    <description><![CDATA[
      Plugin to filter out Kotlin componentN() methods
    ]]></description>

    <idea-version since-build="172.0"/>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <completion.contributor language="JAVA"
                                order="first"
                                implementationClass="ComponentNFilteringContributor"/>
    </extensions>

</idea-plugin>